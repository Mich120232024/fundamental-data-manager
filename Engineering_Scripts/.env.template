# Cosmos DB Configuration
# Copy this file to .env and fill in your actual values

# Cosmos DB endpoint (must be in format: https://<account-name>.documents.azure.com)
COSMOS_ENDPOINT=https://your-cosmos-account.documents.azure.com

# Cosmos DB key (primary or secondary key from Azure Portal)
COSMOS_KEY=your-cosmos-db-key-here

# Database and container names
COSMOS_DATABASE_NAME=your-database-name
COSMOS_CONTAINER_NAME=your-container-name

# Optional: Other configuration
# LOG_LEVEL=INFO