          "parameters": ["symbol", "since", "max", "limit", "callback", "filter"],
          "response_format": "JSON with messages and cursor data"
        },
        {
          "path": "/streams/trending",
          "full_url": "https://api.stocktwits.com/api/2/streams/trending",
          "method": "GET",
          "description": "Get trending symbols and discussions",
          "documentation_link": "https://api.stocktwits.com/developers/docs",
          "parameters": ["limit", "callback"],
          "response_format": "JSON with trending symbols array"
